@model MAPMA_WebClient.BookRef.BookingServicesClient

@{
    ViewBag.Title = "CreateBooking";
}

<h2>CreateBooking</h2>
<form method="post" action="@Url.Action("CreateBooking")">
    Medarbejder nr:<input type="text" name="id" />
    Brugernavn : <input type="text" name="username" />
    Escaperoom Id: <input type="text" name="Id" />
    tid :<input type="datetime" name="BookTime" />
    Antal personer: <input type="text" name="AmountOfPeople" />
    Dato og tid : <input type="datetime" name="BDate"/>
    <input class="Button" type="submit" value="find" />
    
</form>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>BookingServicesClient</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
