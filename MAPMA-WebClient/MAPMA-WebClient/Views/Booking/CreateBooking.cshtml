@model MAPMA_WebClient.BookRef.BookingServicesClient

@{
    ViewBag.Title = "CreateBooking";
}

<h2>CreateBooking</h2>
<form method="post" action="@Url.Action("CreateBooking")">
    Medarbejder nr:<input type="text" name="EmployeeID" /><br /><br />
    Brugernavn : <input type="text" name="username" /><br /><br />
    Escaperoom Id: <input type="text" name="escapeRoomID" /><br /><br />
    tid :<input type="datetime-local" name="BookTime" /><br /><br />
    <select id="timeDropdownlist"></select>
    Antal personer: <input type="text" name="AmountOfPeople" /><br /><br />
    Dato og tid : <input type="datetime-local" name="BDate" /><br /><br />
    <input class="Button" type="submit" value="find" />
</form>



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>BookingServicesClient</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bookingtime.js"></script>
}
